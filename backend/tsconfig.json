{
  "compilerOptions": { 
    "target": "es2016",                                  
    "module": "commonjs",                                 
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,  
    "sourceMap": true,        
    "strict": true,                                     
    "skipLibCheck": true,
    "outDir": "dist"                               
  },
  "lib":[
    "es2015"
  ],
  "include":[
    "src/**/*"
  ]
}
